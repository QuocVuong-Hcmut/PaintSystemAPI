<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Nodejs basic</title>
    <link rel="stylesheet" href="/css/style.css">
    <style>

        </style>
</head>
<body>
    <div class="topnav">
        <a class="active" href="#home">Home</a>
        <a href="#news">News</a>
        <a href="#contact">Contact</a>
        <a href="#about">About</a>
      </div>
      
      <div style="padding:0 16px">
        <h2>List User</h2>
        <h2><%= dataUser %></h2>
        <table id="customers">
            
            <tbody>
                <tr>
                    <th>ID</th>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Action</th>
                </tr>
                <% for(let i=0; i < dataUser.length; i++) { %>
                    <tr>
                      <td><%= dataUser[i].id %></td>
                      <td><%= dataUser[i].firstName %></td>
                      <td><%= dataUser[i].lastName %></td>
                      <td>
                        <a href="./user/detail/<%= dataUser[i].id %>">Detail</a>
                        <a href="./user/detail/<%= dataUser[i].id %>">Change</a>
                        <a href="./user/detail/<%= dataUser[i].id %>">Delete</a>
                      </td>
                    </tr>
                 <% } %>

            </tbody>
          </table>

      </div>
</body>
</html>